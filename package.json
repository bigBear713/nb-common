{
  "name": "nb-common-demo",
  "version": "17.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start:lib": "ng build --watch nb-common",
    "build:lib": "ng build nb-common && npm run copy:readmeCn && npm run copy:changelog && npm run copy:license",
    "build:analyze": "npm run build -- --source-map && npm run view:analyze",
    "view:analyze": "node_modules/.bin/source-map-explorer dist/nb-common-demo/*",
    "test:lib": "ng test nb-common",
    "deploy": "ng build --output-path=docs --base-href=/nb-common/ && cp docs/index.html docs/404.html",
    "publish:lib": "npm run build:lib && cd ./dist/nb-common && npm publish --access=public",
    "copy:readmeCn": "cp projects/nb-common/README.CN.md dist/nb-common/",
    "copy:changelog": "cp CHANGELOG.* dist/nb-common/",
    "copy:license": "cp LICENSE dist/nb-common/",
    "pack:lib": "cd ./dist/nb-common && npm pack"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.3",
    "@angular/common": "^17.0.3",
    "@angular/compiler": "^17.0.3",
    "@angular/core": "^17.0.3",
    "@angular/forms": "^17.0.3",
    "@angular/platform-browser": "^17.0.3",
    "@angular/platform-browser-dynamic": "^17.0.3",
    "@angular/router": "^17.0.3",
    "dayjs": "^1.11.10",
    "lodash-es": "^4.17.21",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.1",
    "@angular/cli": "^17.0.1",
    "@angular/compiler-cli": "^17.0.3",
    "@types/jasmine": "~3.8.0",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^17.0.1",
    "source-map-explorer": "^2.5.3",
    "typescript": "~5.2.2"
  }
}